# PostgreSQL Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=n8n

# n8n Configuration - REQUIRED
N8N_ENCRYPTION_KEY=your-32-character-encryption-key-here
N8N_PROTOCOL=https
WEBHOOK_URL=https://your-app.up.railway.app
N8N_EDITOR_BASE_URL=https://your-app.up.railway.app

# Basic Authentication - REQUIRED
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-secure-password-here

# User Management
N8N_USER_MANAGEMENT_DISABLED=false

# Email Configuration (Optional)
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=smtp.example.com
N8N_SMTP_PORT=587
N8N_SMTP_USER=your-email@example.com
N8N_SMTP_PASS=your-email-password
N8N_SMTP_SENDER=n8n@example.com
N8N_SMTP_SSL=false

# Execution Settings (Optional)
EXECUTIONS_TIMEOUT=3600
EXECUTIONS_TIMEOUT_MAX=7200
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_ON_PROGRESS=true
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true

# Security & Diagnostics (Optional)
N8N_DIAGNOSTICS_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=true
N8N_PERSISTED_BINARY_DATA_TTL=1440

# Binary Data Storage (Optional - for S3)
# N8N_DEFAULT_BINARY_DATA_MODE=s3
# N8N_BINARY_DATA_S3_BUCKET=your-bucket
# N8N_BINARY_DATA_S3_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-key
# AWS_SECRET_ACCESS_KEY=your-secret
